{
  "name": "rfm9x",
  "version": "0.1.0",
  "description": "A js module for interacting with adafruit RFM9x Module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patFish/rfm9x.git"
  },
  "keywords": ["rfm9x", "adafruit", "lora"],
  "author": "patFish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patFish/rfm9x/issues"
  },
  "homepage": "https://github.com/patFish/rfm9x#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
